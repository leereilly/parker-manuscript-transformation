      #content-latest a:hover { text-decoration: underline; } 
      #content-old a:link, #content-old a:visited, #content-old a:active { color: #fff; text-decoration: none; outline: none; font-weight: bold; } 
      #content-old a:hover { text-decoration: underline; } 
      .mainText { font-family: 'lucida grande', tahoma, verdana; font-size:11px; color:#ddd; line-height:1.6; }
      .bold { font-weight: bold; }
      .last-li { padding-bottom: 6px; }
      .hdr { font-weight: bold; letter-spacing: 3px; font-size: 15px; text-transform: uppercase; color: #444; } 
      .gray { color: #888; }
      .icon-box { background-color: #fff; border: 1px solid #ccc; margin-top: 10px; padding: 20px; } 
      li { padding: 0 0 0 12px; margin: 0; }
    </style>
  </head>
